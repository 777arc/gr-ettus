<?xml version="1.0"?>
<block>
  <name>RFNoC: FIR</name>
  <key>uhd_rfnoc_streamer_fir</key>
  <import>import ettus</import>
  <make>ettus.rfnoc_fir_cci(
    $taps,
    self.device3,
    #if $block_index() then $block_index, else -1#
    #if $device_index() then $device_index, else -1#
)
  </make>
  <callback>set_taps($taps)</callback>

  <param>
    <name>Filter Taps</name>
    <key>taps</key>
    <value>[14614,-69849,-203468,-216117,-7860,194302,81889,-216542,-207219,195672,372287,-93615,-558778,-131378,743447,557343,-899809,-1447471,1004420,5224804,7347329,5224804,1004420,-1447471,-899809,557343,743447,-131378,-558778,-93615,372287,195672,-207219,-216542,81889,194302,-7860,-216117,-203468,-69849,14614]</value>
    <type>int_vector</type>
  </param>

  <param>
    <name>FIR Select</name>
    <key>block_index</key>
    <value>-1</value>
    <type>int</type>
    <hide>#if $block_index &lt; 0 then 'part' else 'none'#</hide>
    <tab>RFNoC Config</tab>
  </param>

  <param>
    <name>Device Select</name>
    <key>device_index</key>
    <value>-1</value>
    <type>int</type>
    <hide>#if $device_index &lt; 0 then 'part' else 'none'#</hide>
    <tab>RFNoC Config</tab>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
    <domain>rfnoc</domain>
  </sink>

  <source>
    <name>out</name>
    <type>complex</type>
    <domain>rfnoc</domain>
  </source>
</block>
